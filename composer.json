{
    "name": "marshmallow/nova-translation",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "joedixon/laravel-translation": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Marshmallow\\NovaTranslation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\NovaTranslation\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
